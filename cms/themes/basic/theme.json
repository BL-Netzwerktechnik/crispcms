{
  "name": "full",
  "author": "Justin Ren√© Back <j.back@jrbit.de>",
  "description": "Example theme.json with all possible parameters prefilled",
  "type": 1,
  "strict_autoloading": true,
  "autoload": [
    "includes/class",
    "includes/class/MyClass.php",
    "includes/vendor"
  ],
  "hookFile": "ThemeHook.php",
  "onInstall": {
    "createTranslationKeys": "translations/",
    "createKVStorageItems": {
      "site_name": "My Awesome Site"
    }
  },
  "onUninstall": {
    "deleteData": true
  }
}
