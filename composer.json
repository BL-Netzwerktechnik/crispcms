{
  "name": "jrbit/crispcms",
  "description": "Dynamic CMS",
  "autoload": {
        "psr-4": {
            "crisp\\": "cms/jrbit/class/crisp/"
      }
  },
  "repositories": [
    {
      "type": "composer", 
      "url": "https://gitlab.jrbit.de/api/v4/group/48/-/packages/composer/packages.json"
    }
  ],
  "require": {
    "twig/twig": "3.4.3",
    "nette/utils": "3.0",
    "beberlei/assert": "^3.3",
    "league/oauth2-client": "^2.6",
    "phpmailer/phpmailer": "^6.4",
    "ext-curl": "*",
    "ext-pdo": "*",
    "ext-gd": "*",
    "ext-intl": "*",
    "ext-gettext": "*",
    "ext-openssl": "*",
    "sentry/sdk": "^3.5",
    "guzzlehttp/guzzle": "^7.4",
    "symfony/cache": "^5.3",
    "vlucas/phpdotenv": "^5.3",
    "jrbit/oauth2-keycloak": "dev-master",
    "aws/aws-sdk-php": "^3.185",
    "flagsmith/flagsmith-php-client": "2.1.1",
    "jrb-it/php-cli": "^1.0",
    "ext-fileinfo": "*",
    "nesbot/carbon": "^2.67",
    "geoip2/geoip2": "^2.13",
    "fakerphp/faker": "^1.23",
    "maltyxx/images-generator": "^1.0",
    "phroute/phroute": "^2.2",
    "monolog/monolog": "^3.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "friendsofphp/php-cs-fixer": "^3.35"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
